{
  "name": "jobtrip-frontend",
  "private": true,
  "version": "0.1.0",
  "description": "JobTrip 职途助手前端应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
